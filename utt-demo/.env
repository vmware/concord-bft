# Images
UTT_DEMO_REPLICA_IMAGE=concordbft/utt-demo-replica
UTT_DEMO_REPLICA_IMAGE_TAG=latest

UTT_DEMO_WALLET_IMAGE=concordbft/utt-demo-wallet
UTT_DEMO_WALLET_IMAGE_TAG=latest

UTT_DEMO_PAYMENT_SERVICE_IMAGE=concordbft/utt-demo-payment-service
UTT_DEMO_PAYMENT_SERVICE_IMAGE_TAG=latest

# Storage
UTT_DEMO_VOLUME_LOG=./run/logs:/concord/logs
UTT_DEMO_VOLUME_CONFIG=./run/config:/concord/config:ro
UTT_DEMO_VOLUME_ROCKSDB=./run/rocksdb:/concord/rocksdb

# Network
# All IP addresses and ports must match the net_docker.txt config file

# This is the port for BFT services (bft client, replicas)
UTT_DEMO_BFT_PORT=3710

# This is the port for the public API (wallet, payment service)
UTT_DEMO_PUBLIC_API_PORT=3712

UTT_DEMO_SUBNET=172.25.0.0/16

UTT_DEMO_REPLICA_1_IP=172.25.0.10
